{
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.formatOnSave": true,
  "eslint.nodePath": "./node_modules",
  "eslint.probe": [
    "javascript"
  ],
  "eslint.run": "onType",
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.trace.server": "verbose",
  "cSpell.words": [
    "linq",
    "smbx",
    "teascript"
  ]
}